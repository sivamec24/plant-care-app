{
  "name": "plantcareai",
  "version": "1.0.0",
  "description": "AI-powered plant care assistant with weather-aware reminders",
  "scripts": {
    "build:css": "npx tailwindcss -i ./app/static/src/input.css -o ./app/static/css/output.css --minify",
    "watch:css": "npx tailwindcss -i ./app/static/src/input.css -o ./app/static/css/output.css --watch",
    "dev": "npm run watch:css"
  },
  "keywords": [
    "plant-care",
    "tailwindcss",
    "flask"
  ],
  "author": "EDH Dev",
  "license": "MIT",
  "devDependencies": {
    "@tailwindcss/forms": "^0.5.7",
    "@tailwindcss/typography": "^0.5.10",
    "baseline-browser-mapping": "^2.10.0",
    "tailwindcss": "^3.4.1"
  }
}
